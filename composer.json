    ]
  },
  "require-dev": {
        "phpstan/phpstan": "^0.12.91",
        "phpunit/phpunit": "^8.5.14"
  },
    "autoload": {
        "psr-4": {"ksfraser\\origin\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"ksfraser\\origin\\": "tests/"}
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "phpstan": "@php vendor/bin/phpstan analyse"
    }

}

