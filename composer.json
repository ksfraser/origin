{
  "name": "ksfraser/origin",
  "description": "A set of base classes for my framework.",
  "homepage": "https://github.com/ksfraser/origin",
  "type": "library",
  "authors": [
    {
      "name": "Kevin Fraser",
      "email": "kevin@ksfraser.ca",
      "role": "lead",
      "homepage": "https://ksfraser.ca"
    }
  ],
  "minimum-stability": "dev",
  "require": {
  },
  "keywords": [
      "ksfraser"
  ],
  "support": {
      "issues": "https://github.com/ksfraser/origin/issues",
